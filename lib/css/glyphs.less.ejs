/*
 * Plantilla para la generación de la hoja de estilos de definición de iconos de la librería UIX.
 * @author angel.teran
 */
@font-face {
    font-family: "glyphs";
    src:url("../fonts/glyphs/glyphs<%=timestamp%>.eot");
    src:url("../fonts/glyphs/glyphs<%=timestamp%>.eot#iefix") format("embedded-opentype"),
    url("../fonts/glyphs/glyphs<%=timestamp%>.ttf") format("truetype"),
    url("../fonts/glyphs/glyphs<%=timestamp%>.woff") format("woff"),
    url("../fonts/glyphs/glyphs<%=timestamp%>.svg#glyphs") format("svg");
    font-weight: normal;
    font-style: normal;
}
.icon {
    display: inline-block;
    font-family: "glyphs" !important;
    font-size: 24px;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1 !important;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
<%
    for (var i = 0, code; i < glyphs.length; i++) {
        code = glyphs[i].unicode[0].charCodeAt(0).toString(16); %>
.icon-<%=glyphs[i].name%>:before {
    content: "\<%=code%>";
}<%
    } %>
